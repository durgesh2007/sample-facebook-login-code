        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "verbose": true,
    "preset": "jest-expo",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
