{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build:android": "expo build:android",
    "test": "jest --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --output=html --outputPath=./report.html",
    "web": "expo start --web",
    "eject": "expo eject"
  },
